{{end}}
import "protoc-gen-openapiv2/options/annotations.proto";

option (grpc.gateway.protoc_gen_openapiv2.options.openapiv2_swagger) = {
    info: {
        title: "{{.GoModule}}";
        version: "1.0";
        description: "Boilerplate code generated by **xo-grpc**. Modify `proto/typespb.proto` file then run '*compile.sh*' script on proto directory to change this description.";
        contact: {
            name: "xo-grpc";
            url: "https://github.com/walterwanderley/xo-grpc";
        };
    };
};
{{ range $key, $value := .Messages}}{{if not $value.ElementType}}
message {{$key}} {
{{$value.ProtoAttributes -}}
}{{end}}
{{end -}}